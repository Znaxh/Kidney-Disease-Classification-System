# Production environment variables for Vercel deployment

# Railway backend URL
VITE_API_URL=https://kidney-disease-classification-system-production.up.railway.app

# App Configuration
VITE_APP_NAME=KidneyAI
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false

# File Upload Limits
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/jpg,image/png
